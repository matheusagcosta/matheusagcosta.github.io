section#projects.community
  h2 Projects
  p I'm very close to communities helping organizing events, designing resources and pushing code.
  nav.community__nav
    each community, slug in public.templates.community._data
      a.community__link(href="#{ community.url }" target="_blank")
        img.community__icon(src="../../assets/logo-#{ community.icon }-bauru.svg", alt="#{ community.legend }")
        span.community__headline #{ slug } Bauru
        small.community__legend #{ community.desc }
